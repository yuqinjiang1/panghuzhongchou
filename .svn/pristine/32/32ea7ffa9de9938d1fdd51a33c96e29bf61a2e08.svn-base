package com.xykj.crowdfunding.pojo;

public class User {
	//用户id
	private String userId;
	//角色id
	private String roleId;
	//用户姓名
	private String name;
	//用户密码
	private String password;
	//用户手机号
	private String phone;
	//交易密码
	private String traderPassword;
	//邮箱
	private String email;
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getRoleId() {
		return roleId;
	}
	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getTraderPassword() {
		return traderPassword;
	}
	public void setTraderPassword(String traderPassword) {
		this.traderPassword = traderPassword;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public User(String userId, String roleId, String name, String password,
			String phone, String traderPassword, String email) {
		super();
		this.userId = userId;
		this.roleId = roleId;
		this.name = name;
		this.password = password;
		this.phone = phone;
		this.traderPassword = traderPassword;
		this.email = email;
	}
	public User(String roleId, String name, String password, String phone,
			String traderPassword, String email) {
		super();
		this.roleId = roleId;
		this.name = name;
		this.password = password;
		this.phone = phone;
		this.traderPassword = traderPassword;
		this.email = email;
	}
	public User() {
		super();
	}
	@Override
	public String toString() {
		return "User [userId=" + userId + ", roleId=" + roleId + ", name="
				+ name + ", password=" + password + ", phone=" + phone
				+ ", traderPassword=" + traderPassword + ", email=" + email
				+ "]";
	}

	

}
