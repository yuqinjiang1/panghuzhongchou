<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>修改绑定邮箱</title>
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/easyui/themes/default/easyui.css">
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/easyui/themes/icon.css">
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/jquery.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/jquery.easyui.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/locale/easyui-lang-zh_CN.js"></script>
</head>
<body>
	<div class="easyui-panel" title="修改绑定邮箱" style="width:400px;padding:30px 60px">
		<div style="margin-bottom:20px">
			<div>原绑定邮箱：</div>
			<input class="easyui-textbox"  style="width:100%;height:32px">
		</div>
		<div style="margin-bottom:20px">
			<div>新绑定邮箱：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
		</div>
		<div style="margin-bottom:20px">
			<div>确认新绑定邮箱：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
		</div>
		<div style="margin-bottom:20px">
			<div>手机验证码：</div>
			<input class="easyui-textbox" style="width:58%;height:32px">
			<a href="#" class="easyui-linkbutton" onclick="#">发送手机验证码</a>
		</div>
		<div style="margin-bottom:20px">
			<div>输入交易密码：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
			<p style="color:#5AB4E2" >（交易密码默认是登录密码）</p>
		</div>
		<div style="margin-bottom:20px">
			<div>输入验证码：</div>
			<input class="easyui-textbox" style="width:80%;height:32px">
			<a href="#" class="easyui-linkbutton" onclick="#">7xz2</a>
		</div>
		<div>
			<a href="#" class="easyui-linkbutton" iconCls="icon-ok" style="width:100%;height:32px">确定</a>
		</div>
	</div>
</body>
</html>