<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>修改交易密码</title>
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/easyui/themes/default/easyui.css">
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/easyui/themes/icon.css">
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/jquery.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/jquery.easyui.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/easyui/locale/easyui-lang-zh_CN.js"></script>
</head>
<body>
	<div class="easyui-panel" title="修改交易密码" style="width:400px;padding:30px 60px">
		<div style="margin-bottom:20px">
			<div>旧交易密码：</div>
			<input class="easyui-textbox"  style="width:100%;height:32px">
		</div>
		<div style="margin-bottom:20px">
			<div>新交易密码：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
		</div>
		<div style="margin-bottom:20px">
			<div>确认新交易密码：</div>
			<input class="easyui-textbox" style="width:100%;height:32px" placeholder="6位不连续数字">
		</div>
		<div style="margin-bottom:20px">
			<div>手机号码：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
			<a href="#" class="easyui-linkbutton"  style="width:100%;height:32px">发送手机验证码</a>
		</div>
		<div style="margin-bottom:20px">
			<div>输入手机验证码：</div>
			<input class="easyui-textbox" style="width:100%;height:32px">
			
		</div>
		
		<div>
			<a href="#" onclick="#" class="easyui-linkbutton" iconCls="icon-ok" style="width:100%;height:32px">确定</a>
		</div>
	</div>
</body>
</html>